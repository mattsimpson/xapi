{
    "name": "xabbuh/xapi-serializer",
    "type": "library",
    "description": "Experience API model serializer",
    "keywords": ["xAPI", "Experience API", "Tin Can API", "serializer"],
    "homepage": "https://github.com/php-xapi/xapi-serializer",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Flothmann",
            "homepage": "https://github.com/xabbuh"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "jms/serializer": "~0.16",
        "xabbuh/xapi-model": "~1.0"
    },
    "require-dev": {
        "xabbuh/xapi-data-fixtures": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Xabbuh\\XApi\\Serializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xabbuh\\XApi\\Serializer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
